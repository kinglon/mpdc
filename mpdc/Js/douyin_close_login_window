var jsResult = {};
jsResult['fun'] = 'close_login_window';
var loginPanel = document.getElementById('login-pannel');
if (loginPanel && loginPanel.style.display !== 'none')
{
    jsResult["login_window_visible"] = '1';
    var closeBtns = loginPanel.getElementsByClassName('dy-account-close');
    if (closeBtns.length > 0)
    {
        closeBtns[0].click();
    }
}
jsResult;